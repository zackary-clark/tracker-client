  "license": "MIT",
  "scripts": {
    "analyze": "webpack --json > stats.json",
    "build": "webpack -p",
    "ship": "yarn build && cd dist && git add --all && git commit -m \"Release at $(date)\" && git push",
    "start": "webpack-dev-server --open",
