{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    },
    "rules": {
        "no-trailing-whitespace": [true, "ignore-blank-lines"],
        "ordered-imports": false,
        "max-line-length": [true, 140],
        "object-literal-sort-keys": false,
        "arrow-parens": false
    },
    "rulesDirectory": []
}